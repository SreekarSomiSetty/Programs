import java.util.*;
class g
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        
        int n=sc.nextInt();
        int arr[]=new int[n];
        List<Integer> lt=new ArrayList<>();
        HashSet<Integer> s=new HashSet<>();
        for(int i=0;i<n;i++)
        {
            arr[i] = sc.nextInt();
            lt.add(arr[i]);
            s.add(arr[i]);
        }
        System.out.println(lt);
        System.out.println(s);
        
    }
}
