import java.util.Scanner;

class g
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        
        String str = sc.nextLine();
        if(str.contains("ee"))
        {
            System.out.println("True");
        }
        else
        {
            System.out.println("False");
        }
    }
}
